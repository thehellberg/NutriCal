{
  "expo": {
    "newArchEnabled": true,
    "experiments": {
      "typedRoutes": true
    },
    "name": "NutriCal",
    "slug": "nutrical",
    "version": "0.0.1",
    "platforms": [
      "ios",
      "android"
    ],
    "notification": {
      "androidMode": "collapse"
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "infoPlist": {
        "ExpoLocalization_supportsRTL": true,
        "LSMinimumSystemVersion": "13.0",
        "ExpoLocalization_forcesRTL": false,
        "ITSAppUsesNonExemptEncryption": false
      },
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.nutrihealth.nutrical",
      "privacyManifests": {
        "NSPrivacyCollectedDataTypes": [
          {
            "NSPrivacyCollectedDataType": "NSPrivacyCollectedDataTypeCrashData",
            "NSPrivacyCollectedDataTypeLinked": false,
            "NSPrivacyCollectedDataTypeTracking": false,
            "NSPrivacyCollectedDataTypePurposes": [
              "NSPrivacyCollectedDataTypePurposeAppFunctionality"
            ]
          },
          {
            "NSPrivacyCollectedDataType": "NSPrivacyCollectedDataTypePerformanceData",
            "NSPrivacyCollectedDataTypeLinked": false,
            "NSPrivacyCollectedDataTypeTracking": false,
            "NSPrivacyCollectedDataTypePurposes": [
              "NSPrivacyCollectedDataTypePurposeAppFunctionality"
            ]
          },
          {
            "NSPrivacyCollectedDataType": "NSPrivacyCollectedDataTypeOtherDiagnosticData",
            "NSPrivacyCollectedDataTypeLinked": false,
            "NSPrivacyCollectedDataTypeTracking": false,
            "NSPrivacyCollectedDataTypePurposes": [
              "NSPrivacyCollectedDataTypePurposeAppFunctionality"
            ]
          }
        ],
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategorySystemBootTime",
            "NSPrivacyAccessedAPITypeReasons": [
              "35F9.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryFileTimestamp",
            "NSPrivacyAccessedAPITypeReasons": [
              "C617.1"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.nutrihealth.nutrical"
    },
    "extra": {
      "eas": {
        "projectId": "0b8a496c-f7c8-4d87-abc1-1476d4e4c9f2"
      },
      "supportsRTL": true,
      "forcesRTL": false
    },
    "owner": "nutrical",
    "plugins": [
      [
        "expo-localization",
        {
          "supportedLocales": [
            "en",
            "ar"
          ]
        }
      ],
      [
        "expo-router",
        {
          "origin": "https://example.com/",
          "sitemap": false
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-apple-authentication",
      [
        "expo-font",
        {
          "fonts": [
            "../../node_modules/@expo-google-fonts/rubik/Rubik_300Light.ttf",
            "../../node_modules/@expo-google-fonts/rubik/Rubik_400Regular.ttf",
            "../../node_modules/@expo-google-fonts/rubik/Rubik_500Medium.ttf",
            "../../node_modules/@expo-google-fonts/rubik/Rubik_600SemiBold.ttf",
            "../../node_modules/@expo-google-fonts/rubik/Rubik_700Bold.ttf",
            "../../node_modules/@expo-google-fonts/rubik/Rubik_800ExtraBold.ttf"
          ]
        }
      ],
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "The app accesses your photos to let you share them with the community.",
          "microphonePermission": "The app accesses your microphone to allow you to send voice memos",
          "recordAudioAndroid": false
        }
      ],
      "react-native-ble-plx",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "@sentry/react-native",
        {
          "organization": "sentry",
          "project": "sentry",
          "url": "https://sentry.io"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      "expo-sqlite",
      "expo-notifications"
    ],
    "scheme": "nutrical",
    "updates": {
      "enabled": false
    }
  }
}